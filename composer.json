{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0.8",
        "league/glide": "^1.1@dev",
        "pda/pheanstalk": "^3.1@dev",
        "ramsey/uuid-doctrine": "^1.4",
        "sensio/framework-extra-bundle": "^4.0@dev",
        "symfony/asset": "3.4.x-dev",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.0@dev",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^3.3",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "3.4.x-dev",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^2.4@dev",
        "fzaninotto/faker": "^1.8@dev",
        "phpunit/phpunit": "^6.5@dev",
        "symfony/debug-bundle": "3.4.x-dev",
        "symfony/dotenv": "^3.3",
        "symfony/flex": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "mkdir -p var/uploads"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BWFSDBN3DHNTPQC7R56YDP77",
            "allow-contrib": false
        }
    }
}
